    "pop": "g",
    "plant_a": "a",
    "security": "n g",
    "service_sg": "s g",
    "aam_battle": "n f g",
    "activity_nga": "n g a",
    "mood": "g",
    "nbmood": "n",
    "nbur": "n"
  }
]}    

