    }


    test CREATE_RAW-2.1 {nbhood is created, controller is NONE} -body {
        ted orderx NBHOOD:CREATE:RAW $rawnb1
        set parms [join [dict keys $rawnb1] ","]
        ted querylist "SELECT $parms FROM nbhoods"
    } -cleanup {
        cleanup
    } -result {
n             RAW1
longname      Raw1
refpoint      30 30
polygon       -90 -180 90 -180 90 180 -90 180
urbanization  URBAN
controller    
pcf           1.0
local         1
    }

    test CREATE_RAW-2.2 {nbhood is created, controller is given} -setup {
        ted create JOE
    } -body {
        ted orderx NBHOOD:CREATE:RAW [dict merge $rawnb1 {controller JOE}]
        set parms [join [dict keys $rawnb1] ","]
        ted querylist "SELECT $parms FROM nbhoods"
    } -cleanup {
        cleanup
    } -result {
n             RAW1
longname      Raw1
refpoint      30 30
polygon       -90 -180 90 -180 90 180 -90 180
urbanization  URBAN
controller    JOE
pcf           1.0
