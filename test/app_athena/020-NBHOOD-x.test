    } -body {
        ted orderx -reject NBHOOD:UPDATE n NB1 urbanization NONESUCH
    } -cleanup {
        cleanup
    } -result {
        urbanization {invalid value "NONESUCH", should be one of: ISOLATED, RURAL, SUBURBAN, URBAN}
    }

    test UPDATE-1.3 {invalid controller} -setup {
        ted orderx NBHOOD:CREATE $nb1
    } -body {
        ted orderx -reject NBHOOD:UPDATE n NB1 controller NONESUCH
    } -cleanup {
        cleanup
    } -result {
