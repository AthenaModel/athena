    }

    test CREATE_RAW-2.3 {longname defaults to ID} -body {
        ted orderx NBHOOD:CREATE:RAW [dict merge $rawnb1 {longname ""}]

        rdb onecolumn {SELECT longname FROM nbhoods}
    } -cleanup {
