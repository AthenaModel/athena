    } -result {RAW1}

    test CREATE_RAW-2.4 {Effects} -body {
        ted notifier bind ::rdb <nbhoods>
        ted orderx NBHOOD:CREATE:RAW $rawnb1
        ted notifier received
    } -cleanup {
        cleanup
    } -result {
