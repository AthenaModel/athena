    test DEPLOYED_TO-1.1 {construct} -setup {
        setup
    } -body {
        ::gofer::FRCGROUPS::DEPLOYED_TO construct any {NB1 NB2}
    } -cleanup {
        cleanup
    } -result {anyall ANY nlist {NB1 NB2}}

    test DEPLOYED_TO-2.1 {validate} -setup {
        setup
    } -body {
        ::gofer::FRCGROUPS::DEPLOYED_TO validate {anyall any nlist {NB1 NB2}}
    } -cleanup {
        cleanup
    } -result {anyall ANY nlist {NB1 NB2}}

    test DEPLOYED_TO-3.1 {narrative} -body {
        ::gofer::FRCGROUPS::DEPLOYED_TO narrative {anyall ANY nlist NB1}
    } -match glob -result {force groups that are deployed to *}

    test DEPLOYED_TO-4.1 {eval} -setup {
        setup
        foreach {n g personnel} {
            NB1 BLUE 1
            NB1 BRIT 1
            NB1 TAL  0
            NB2 TAL  1
        } {
            rdb eval {
                INSERT INTO deploy_ng(n,g,personnel)
                VALUES($n, $g, $personnel)
            }
        }
    } -body {
        ::gofer::FRCGROUPS::DEPLOYED_TO eval {anyall ANY nlist NB1}
    } -cleanup {
        cleanup
    } -result {BLUE BRIT}

