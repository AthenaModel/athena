        setup
    } -body {
        ::gofer::FRCGROUPS::OWNED_BY construct {JOE BOB}
    } -cleanup {
        cleanup
    } -result {alist {JOE BOB}}

    test OWNED_BY-2.1 {validate} -setup {
        setup
    } -body {
        ::gofer::FRCGROUPS::OWNED_BY validate {alist {JOE BOB}}
    } -cleanup {
        cleanup
    } -result {alist {JOE BOB}}

    test OWNED_BY-3.1 {narrative} -body {
        ::gofer::FRCGROUPS::OWNED_BY narrative {alist JOE}
    } -match glob -result {force groups owned by *}

    test OWNED_BY-4.1 {eval} -setup {
        setup
    } -body {
        ::gofer::FRCGROUPS::OWNED_BY eval {alist JOE}
    } -cleanup {
        cleanup
    } -result {BLUE BRIT}

