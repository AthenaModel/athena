        setup
        foreach {g a support} {
            BRIT BOB 0.3
            BRIT JOE 0.3
            BLUE BOB 0.0
            SHIA BOB 0.3
        } {
            rdb eval {
                INSERT INTO support_nga(n,g,a,support)
                VALUES('NB1',$g,$a,$support)
            }
        }
    } -body {
        ::gofer::FRCGROUPS::SUPPORTING_ACTOR eval {anyall ANY alist BOB}
    } -cleanup {
        cleanup
